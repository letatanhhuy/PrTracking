<?xml version="1.0" encoding="utf-8"?>
<ConfigurationSet Name="default_deviceconfig_Si700" version="1.0.0" LastModifiedDate="2016-04-25" xmlns="http://www.motorolasolutions.com/devices/FusionDeviceConfigurationSchema">
  <Configuration Name="default_deviceconfig_Si700">
    <DeviceWide>
      <DeviceModel>N7002</DeviceModel>
      <ProductName>Si700</ProductName>
      <PairedRadioDeviceType>TETRA</PairedRadioDeviceType>
      <PairedRadioConnectionType>Gcai</PairedRadioConnectionType>
      <PairedRadioConnectionTypeWiredSelectable>true</PairedRadioConnectionTypeWiredSelectable>
      <PairedRadioConnectionTypeBluetoothSelectable>true</PairedRadioConnectionTypeBluetoothSelectable>
      <PairedRadioConnectionTypeStandaloneSelectable>true</PairedRadioConnectionTypeStandaloneSelectable>
      <EnableAudioOnlyRecording>true</EnableAudioOnlyRecording>
      <EnableCovertMode>true</EnableCovertMode>
      <EnablePhotoCapture>true</EnablePhotoCapture>
      <EnableTimestamp>false</EnableTimestamp>
      <AllowRecordedVideoPlayback>true</AllowRecordedVideoPlayback>
      <AllowContentPreview>true</AllowContentPreview>
      <IncidentTimeout>15</IncidentTimeout>
      <EnableLeds>true</EnableLeds>
      <EnableAudioWhileRecordingVideo>true</EnableAudioWhileRecordingVideo>
      <EnableAudioWhilePreBuffering>true</EnableAudioWhilePreBuffering>
      <DeviceSharingType>Personal</DeviceSharingType>
      <EnableStatusApplication>true</EnableStatusApplication>
      <EnableMCS>true</EnableMCS>
      <EnableMessaging>true</EnableMessaging>
      <EnablePrivateCallTelephonyKeypad>true</EnablePrivateCallTelephonyKeypad>
      <EnableAdhocUpload>true</EnableAdhocUpload>
    </DeviceWide>
    <Buttons>
      <EmergencyButton>
        <ShortPress>ForwardToRadio</ShortPress>
        <LongPress>ForwardToRadio</LongPress>
      </EmergencyButton>
      <P1>
        <ShortPress>Preview</ShortPress>
        <LongPress>AudioRecord</LongPress>
      </P1>
      <P2>
        <ShortPress>VideoMediaMark</ShortPress>
        <LongPress>CovertMode</LongPress>
      </P2>
    </Buttons>
    <Video>
      <VideoResolution>720</VideoResolution>
      <VideoPreBuffering>0</VideoPreBuffering>
    </Video>
    <Photo>
      <PhotoSize>1280x720</PhotoSize>
      <PhotoEncoding>JPG</PhotoEncoding>
    </Photo>
    <Activities>
      <Activity>
        <Event>Emergency</Event>
        <Action>StartVideoRecording</Action>
      </Activity>
    </Activities>
  </Configuration>
</ConfigurationSet>
